$pA = "C:\path\to\original\"
$pB = "c:\path\to\backup\"
$tA = "a.log"
$tB = "b.log"

sal -n "gfh" -va Get-FileHash

$a = ls $pA | %{ac -Path "$pA$tA" -va (gfh -a MD5 -pa $_.F | select -exp "Hash")} #it says pasta lmao
$b = ls $pB | %{ac -Path "$pA$tB" -va (gfh -a MD5 -pa $_.F | select -exp "Hash")}

while($?){
    if(!($a = $b)){
    del $pA
    cpi -Path $pB -D $pA}
    sleep -s 30
}
